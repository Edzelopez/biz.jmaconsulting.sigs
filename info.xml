<?xml version="1.0"?>
<extension key="biz.jmaconsulting.sigs" type="module">
  <file>sigs</file>
  <name>Email Signatures</name>
  <description>This extension allows you to upload an image that can be used as a token while sending emails in order to display your signature at the end of each email.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>JMA Consulting</author>
    <email>joe.murray@jmaconsulting.biz</email>
  </maintainer>
  <releaseDate>2014-01-21</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>4.4</ver>
  </compatibility>
  <comments></comments>
  <civix>
    <namespace>CRM/Sigs</namespace>
  </civix>
</extension>
